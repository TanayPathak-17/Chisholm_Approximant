Cell["\<\
Finding the CA of Li2,2. We have added 1+x+y to the series of Li2,2[x-y,x+y] \
to find the CA and subtracted the same from the CA. \
\>", "Text",
 CellChangeTimes->{{3.947550332631051*^9, 3.947550404782555*^9}, {
  3.947550576169444*^9, 
  3.9475505803688517`*^9}},ExpressionUUID->"36be4208-ccd1-493f-8e84-\
dc9fe40a9d35"],

Cell[BoxData[
 RowBox[{
  RowBox[{"CaLi00", "=", 
   RowBox[{"Monitor", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Block", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"order", "=", "i"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"order", ",", 
           RowBox[{"AbsoluteTiming", "[", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"ChisholmD", "[", 
                RowBox[{
                 RowBox[{"1", "+", " ", "x", " ", "+", " ", "y", "+", " ", 
                  RowBox[{"Li22def", "[", 
                   RowBox[{
                    RowBox[{"x", "-", "y"}], ",", 
                    RowBox[{"x", "+", "y"}], ",", 
                    RowBox[{"2", " ", "order"}]}], "]"}]}], ",", 
                 RowBox[{"{", 
                  RowBox[{"0", ",", "0", ",", "order"}], "}"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"x", ",", "y"}], "}"}]}], "]"}], "-", 
               RowBox[{"(", 
                RowBox[{"1", "+", "x", " ", "+", " ", "y"}], ")"}]}], ")"}], "/.", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"x", "->", "1"}], ",", 
               RowBox[{"y", "->", "0"}]}], "}"}]}], "]"}], ",", 
           RowBox[{"AbsoluteTiming", "[", 
            RowBox[{"Li22def", "[", 
             RowBox[{"1", ",", "1", ",", " ", 
              RowBox[{
               RowBox[{"2", "order"}], "+", "1"}]}], "]"}], "]"}]}], "}"}]}], 
        "]"}], ",", 
