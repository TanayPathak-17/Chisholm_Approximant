Cell[BoxData[
 RowBox[{
  RowBox[{"Li22def", "[", 
   RowBox[{"x_", ",", "y_", ",", "Nmax_"}], "]"}], ":=", " ", 
  RowBox[{"Sum", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"x", "^", "i"}], " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"x", " ", "y"}], ")"}], "j"], "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"i", "+", "j"}], ")"}], "^", "2"}], " ", 
        RowBox[{"j", "^", "2"}]}], ")"}]}]}], ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", "1", ",", "Nmax"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"j", ",", "1", ",", "Nmax"}], "}"}]}], "]"}], " "}]], "Input",
 CellChangeTimes->{{3.947548314565147*^9, 3.947548393803488*^9}, {
   3.947548594959672*^9, 3.947548597165346*^9}, {3.9475490664319*^9, 
